/**index.wxss**/
.left-side {
  width: 200rpx;
  position: absolute;
  background: #f5f5f5;
  color: #727271;
  bottom: 0;
  top: 0;
}
.left-side .list {
  text-align: center;
  padding: 10rpx 20rpx;
}
.left-side .list.active {
  color: #3c3c3c;
  background: #fff;
}
.main {
  width: calc(100% - 200rpx);
  position: absolute;
  bottom: 0;
  top: 0;
  left: 200rpx;
}